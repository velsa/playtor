{
  "main": "index.html",
  "name": "playtor",
  "description": "Playtor - The Magic Magnet Streamer",
  "version": "0.0.1",
  "keywords": [ "torrent", "player", "magnet" ],
  "window": {
    "show":       false,
    "position":   "center",
    "resizable":  false,
    "show_in_taskbar": false,
    "width":      500,
    "height":     500,
    "icon":       "assets/icon/64x64.png",
    "min_width":  400,
    "min_height": 200,
    "max_width":  800,
    "max_height": 600
  },
  "webkit": {
    "plugin": true
  },
  "no-edit-menu": true,
  "dependencies": {
    "read-torrent": "~1.0.0",
    "windows-no-runnable": "0.0.6"
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-curl": "~2.0.2",
    "grunt-zip": "~0.15.0",
    "grunt-shell": "~0.7.0",
    "grunt-node-webkit-builder": "~0.1.21",
    "load-grunt-tasks": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-clean": "~0.5.0"
  }
}
